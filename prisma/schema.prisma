datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id              String   @id @default(cuid())
  email           String   @unique
  username        String?
  lastDayPlayed   DateTime @updatedAt
  blurryChampions Boolean  @default(true)
}
